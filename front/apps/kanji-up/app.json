{
  "expo": {
    "name": "kanji-up",
    "slug": "kanji-up",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kanjiup",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#851a1b"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.aem.kanjiup",
      "requireFullScreen": true,
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#851a1b"
      },
      "package": "com.aem.kanjiup"
    },
    "web": {
      "name": "KanjiUp V2",
      "backgroundColor": "#851a1b",
      "display": "fullscreen",
      "orientation": "portrait",
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9a61f40d-6106-45a6-9ecf-5522bad073a4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9a61f40d-6106-45a6-9ecf-5522bad073a4"
    }
  }
}

{
  "name": "kanji-up",
  "version": "0.0.3",
  "description": "## Gateway App",
  "scripts": {
    "start:gateway": "cd ./kanji-up/ && cp .env.development .env && yarn start",
    "start:kanji": "cd ./kanji-app/ && cp .env.development .env && yarn start",
    "start:home": "cd ./home-app/ && cp .env.development .env && yarn start",
    "start:search": "cd ./search-app/ && cp .env.development .env && yarn start",
    "start:dev": "concurrently --names \"gateway,kanji,home,search\" \"yarn start:gateway\" \"yarn start:kanji\" \"yarn start:home\" \"yarn start:search\"",
    "build:gateway": "cd ./kanji-up/ && cp .env.production .env && yarn build",
    "build:kanji": "cd ./kanji-app/ && cp .env.production .env && yarn build",
    "build:home": "cd ./home-app/ && cp .env.production .env && yarn build",
    "build:search": "cd ./search-app/ && cp .env.production .env && yarn build",
    "build": "concurrently --names \"gateway,kanji,home,search\" \"yarn build:gateway\" \"yarn build:kanji\" \"yarn build:home\" \"yarn build:search\"",
    "env": "cp env/.env.*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^9.1.0"
  }
}
